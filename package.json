{
  "name": "@tanqory/api-client",
  "version": "0.1.1",
  "description": "A TypeScript client library for the Tanqory API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "echo \"Warning: No tests specified\" && exit 0",
    "test:watch": "echo \"Warning: No tests specified\" && exit 0",
    "test:coverage": "echo \"Warning: No tests specified\" && exit 0",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "tanqory",
    "api",
    "client",
    "typescript",
    "javascript",
    "sdk",
    "rest",
    "http",
    "scoped"
  ],
  "author": {
    "name": "TANQ PTE. LTD.",
    "email": "dev@tanqory.com",
    "url": "https://tanqory.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/tanqory/api-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanqory/api-client.git"
  },
  "bugs": {
    "url": "https://github.com/tanqory/api-client/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@tanqory/site-utils": "^0.0.32",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0"
  }
}
